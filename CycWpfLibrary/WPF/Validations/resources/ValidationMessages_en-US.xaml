<ResourceDictionary x:Class="ValidationMessages_enUS"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:CycWpfLibrary">

  <sys:String x:Key="ValidationMessageNotSimilar">
    Input string should not be similar with 
