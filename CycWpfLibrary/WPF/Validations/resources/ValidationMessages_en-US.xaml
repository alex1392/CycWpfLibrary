<ResourceDictionary x:Class="ValidationMessages_enUS"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:CycWpfLibrary">
  <sys:String x:Key="ValidationMessageNotNull">
    Input should not be null
  </sys:String>
  <sys:String x:Key="ValidationMessageDouble">
    Input number is invalid
  </sys:String>
  <sys:String x:Key="ValidationMessageLogBase">
    Log Base must larger than 0
  </sys:String>
  <sys:String x:Key="ValidationMessageByte">
    Byte must between (0, 255)
  </sys:String>
  <sys:String x:Key="ValidationMessageInRange">
    Input number should be in
  </sys:String>
  <sys:String x:Key="ValidationMessageDate">
    Invalid date
  </sys:String>
  <sys:String x:Key="ValidationMessageFutureDate">
    Future date required
  </sys:String>
  <sys:String x:Key="ValidationMessagePastDate">
    Past date required
  </sys:String>
</ResourceDictionary>